{
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "globals": {
      "beforeEach": {},
      "describe": {}, 
      "expect": {}, 
      "it": {}, 
      "jest": {},
      "window": {}
    },
    "parser": "babel-eslint",
    "plugins": [
      "babel"
    ],
    "rules": {
      "babel/object-curly-spacing": 0,
      "no-param-reassign" : ["error", { "props": false }],
      "no-underscore-dangle": 0
    }
}
